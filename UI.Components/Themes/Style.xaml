                                                   Margin="5,0">
                                            <Rectangle.Fill>
                                                <VisualBrush Stretch="Fill" Visual="{StaticResource appbar_check}" />
                                            </Rectangle.Fill>
                                        </Rectangle>
                                        <TextBlock Grid.Column="1"
                                                   Margin="5,0"
